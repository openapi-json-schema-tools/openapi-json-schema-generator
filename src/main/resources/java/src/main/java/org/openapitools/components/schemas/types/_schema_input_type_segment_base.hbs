Map<String, Object>
                                            {{/with}}
                                        {{/eq}}
                                    {{/eq}}
                                {{/eq}}
                            {{/eq}}
                        {{/eq}}
                    {{/eq}}
                {{/eq}}
            {{/eq}}
        {{/each}}
    {{else}}
        {{#eq types.size 2}}
            {{#contains types "null"}}
                {{! nullable types }}
                {{#each types}}
                    {{#eq this "boolean"}}
Boolean
                    {{else}}
                        {{#eq this "string"}}
                            {{#eq format "binary"}}
{{! todo binary types}}
String
                            {{else}}
String
                            {{/eq}}
                        {{else}}
                            {{#eq this "number"}}
                                {{#eq ../format null}}
Number
                                {{else}}
                                    {{#eq ../format "float"}}
Float
                                    {{else}}
                                        {{#eq ../format "double"}}
Double
                                        {{else}}
                                            {{#eq ../format "int32"}}
Integer
                                            {{else}}
                                                {{#eq ../format "int64"}}
Long
                                                {{/eq}}
                                            {{/eq}}
                                        {{/eq}}
                                    {{/eq}}
                                {{/eq}}
                            {{else}}
                                {{#eq this "integer"}}
                                    {{#eq ../format null}}
Long
                                    {{else}}
